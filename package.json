{
  "name": "typescript-boilerplate",
  "version": "1.3.5",
  "main": "index.js",
  "scripts": {
    "start": "npx tsx ./src/main.ts",
    "lint": "cross-env TIMING=1 npx eslint",
    "lint:fix": "npm run lint -- --fix",
    "lint:debug": "npm run lint -- --debug",
    "format:md": "npx prettier --write ./README.md",
    "typecheck": "npx tsc --noEmit",
    "test": "npx vitest",
    "bump-version": "npx tsx ./scripts/bump-version.script.ts && npm audit fix",
    "parse-llm-response": "npx tsx ./scripts/split-plain-code.ts",
    "format": "npx prettier . --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "typed-dotenv": "^10.0.2"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^6.9.3",
    "@dword-design/eslint-plugin-import-alias": "^6.0.3",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/node": "^22.18.8",
    "@vitest/eslint-plugin": "^1.3.23",
    "cross-env": "^10.1.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-project-structure": "^3.13.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "type-fest": "^5.1.0",
    "typescript-eslint": "^8.46.2",
    "unplugin-swc": "^1.5.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "eslint-config-airbnb-base": {
      "eslint": "^9"
    }
  }
}